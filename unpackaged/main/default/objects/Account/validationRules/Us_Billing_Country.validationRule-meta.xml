<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Us_Billing_Country</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(OR(BillingCountry=&#39;US&#39;,BillingCountry =&#39;USA&#39;,BillingCountry =&#39;United States&#39;, ISBLANK(BillingCountry)))</errorConditionFormula>
    <errorDisplayField>BillingCountry</errorDisplayField>
    <errorMessage>Please Billing Country must be either US, USA, United States or left blank.</errorMessage>
</ValidationRule>
